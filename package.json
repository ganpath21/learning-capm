{
  "name": "cap-bookshop-demo",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^6",
    "express": "^4",
    "hdb": "^0.19.0",
    "@sap/xssec": "^3",
    "passport": "^0"
  },
  "scripts": {
    "start": "cds-serve",
    "h-deploy": "cds deploy --to hana:CAPHANAFE-dev"
  },
  "cds": {
    "build": {
      "tasks": [
        {
          "for": "hana",
          "dest": "./db"
        },
        {
          "for": "node-cf"
        }
      ]
    },
    "requires": {
      "db": {
        "kind": "hana-cloud"
      },
      "auth": {
        "kind": "xsuaa"
      },
      "approuter": {
        "kind": "cloudfoundry"
      }
    }
  }
}
